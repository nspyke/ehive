{
    "name": "nspyke/ehive",
    "description": "EHive API client",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Nik Spijkerman",
            "email": "nikspijkerman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EHive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EHive\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0",
        "internations/http-mock": "0.8.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "monolog/monolog": "Compatible logging library",
        "symfony/cache": "Caching library for most types of caching providers (Memcached, Redis, APCu etc)"
    }
}
